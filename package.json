{
  "name": "postgres-orm",
  "version": "1.0.0",
  "description": "A PostgreSQL ORM with schema parsing capabilities",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:parser": "jest src/parser/__tests__/schemaParser.test.ts -- --silent=false",
    "test:migration:generator": "jest src/migration/__tests__/migrationGenerator.test.ts",
    "start": "node dist/index.js",
    "preview": "ts-node src/preview.ts",
    "preview:migration": "ts-node src/preview-migration.ts",
    "preview:migration:json": "ts-node src/preview-migration.ts -- --format json",
    "preview:migration:sql": "ts-node src/preview-migration.ts -- --format raw",
    "preview:migration:file": "ts-node src/preview-migration.ts -- --output migrations/preview.sql --format raw",
    "preview:migration:help": "ts-node src/preview-migration.ts -- --help"
  },
  "dependencies": {
    "@types/pg": "^8.11.11",
    "pg": "^8.14.1",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1"
  }
}
