{
  "name": "postgres-orm",
  "version": "1.0.0",
  "description": "A PostgreSQL ORM with schema parsing capabilities",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:parser": "jest src/parser/__tests__/*.test.ts",
    "test:parser:schema": "jest src/parser/__tests__/schemaParser.test.ts",
    "test:parser:trigger": "jest src/parser/__tests__/triggerParser.test.ts",
    "test:migration:generator": "jest src/migration/__tests__/migrationGenerator.test.ts",
    "test:migration:runner": "jest src/migration/runner/__tests__/migrationRunner.test.ts",
    "test:migration:integration": "jest --forceExit src/migration/runner/__tests__/integrationTests.test.ts",
    "test:migration:integration:verbose": "VERBOSE=true jest --forceExit src/migration/runner/__tests__/integrationTests.test.ts",
    "test:extension": "jest src/migration/__tests__/extensionManagement.test.ts",
    "test:extension:integration": "jest --forceExit src/migration/runner/__tests__/extensionManagementIntegration.test.ts",
    "test:extension:integration:verbose": "VERBOSE=true jest --forceExit src/migration/runner/__tests__/extensionManagementIntegration.test.ts",
    "test:extension:orchestrator": "jest src/migration/extension/__tests__/extensionOrchestrator.test.ts",
    "test:extension:generator": "jest src/migration/__tests__/migrationGeneratorExtensions.test.ts",
    "test:extensions:all": "jest src/migration/extension/__tests__/*.test.ts src/migration/__tests__/migrationGeneratorExtensions.test.ts src/migration/__tests__/extensionManagement.test.ts",
    "test:table:orchestrator": "jest src/migration/table/__tests__/tableOrchestrator.test.ts",
    "test:table:integration": "jest --forceExit src/migration/table/__tests__/tableOrchestratorIntegration.test.ts",
    "test:table:all": "jest src/migration/table/__tests__/*.test.ts",
    "test:relation:orchestrator": "jest src/migration/relation/__tests__/relationOrchestrator.test.ts",
    "test:relation:integration": "jest src/migration/relation/__tests__/relationOrchestratorIntegration.test.ts",
    "test:relation:all": "jest src/migration/relation/__tests__/*.test.ts",
    "test:rls:orchestrator": "jest src/migration/rls/__tests__/rlsOrchestrator.test.ts",
    "test:rls:integration": "jest src/migration/__tests__/migrationGenerator.test.ts -t 'should handle RLS changes'",
    "test:rls:all": "jest src/migration/rls/__tests__/*.test.ts src/migration/__tests__/migrationGenerator.test.ts -t 'RLS'",
    "test:policy:orchestrator": "jest src/migration/rls/__tests__/policyOrchestrator.test.ts",
    "test:policy:integration": "jest src/migration/rls/__tests__/policyOrchestratorIntegration.test.ts",
    "test:policy:all": "jest src/migration/rls/__tests__/policyOrchestrator.test.ts src/migration/rls/__tests__/policyOrchestratorIntegration.test.ts",
    "test:role:orchestrator": "jest src/migration/role/__tests__/roleOrchestrator.test.ts",
    "test:role:integration": "jest src/migration/role/__tests__/roleOrchestratorIntegration.test.ts",
    "test:role:all": "jest src/migration/role/__tests__/roleOrchestrator.test.ts src/migration/role/__tests__/roleOrchestratorIntegration.test.ts",
    "test:trigger:orchestrator": "jest src/migration/trigger/__tests__/triggerOrchestrator.test.ts",
    "test:trigger:integration": "jest --forceExit src/migration/trigger/__tests__/triggerOrchestrator.integration.test.ts",
    "test:trigger:all": "jest src/migration/trigger/__tests__/*.test.ts",
    "test:orchestrators": "jest src/migration/rls/__tests__/*.test.ts src/migration/extension/__tests__/*.test.ts src/migration/table/__tests__/*.test.ts src/migration/enum/__tests__/*.test.ts src/migration/role/__tests__/*.test.ts src/migration/relation/__tests__/*.test.ts src/migration/trigger/__tests__/*.test.ts",
    "start": "node dist/index.js",
    "preview": "ts-node src/preview.ts",
    "preview:migration": "ts-node src/preview-migration.ts",
    "preview:migration:json": "ts-node src/preview-migration.ts -- --format json",
    "preview:migration:sql": "ts-node src/preview-migration.ts -- --format raw",
    "preview:migration:file": "ts-node src/preview-migration.ts -- --output migrations/preview.sql --format raw",
    "preview:migration:help": "ts-node src/preview-migration.ts -- --help",
    "preview:migration:compare": "ts-node src/preview-migration.ts -- --from-schema schema/current.schema --schema schema/new.schema",
    "migrate": "ts-node src/cli/runMigrations.ts",
    "migrate:up": "ts-node src/cli/runMigrations.ts up",
    "migrate:down": "ts-node src/cli/runMigrations.ts down",
    "migrate:status": "ts-node src/cli/runMigrations.ts status",
    "migrate:generate": "ts-node src/cli/runMigrations.ts generate",
    "migrate:generate:full": "ts-node src/cli/runMigrations.ts generate --force-full",
    "migrate:init-state": "ts-node src/cli/runMigrations.ts init-state",
    "example:extension-comparison": "node examples/extension-comparison.js"
  },
  "dependencies": {
    "@types/pg": "^8.11.11",
    "dotenv": "^16.4.7",
    "inquirer": "^8.2.6",
    "pg": "^8.14.1",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1"
  }
}
